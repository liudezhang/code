<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <title><%-book.name%></title>
    <link rel="stylesheet" href="../css/index.css">
    <!-- <link rel="stylesheet" href="../css/font.css"> -->
    <link rel="stylesheet" href="../css/info.css">
    <link rel="stylesheet" href="../css/info.min.css">
    <link rel="stylesheet" href="../css/infotwo.css">
</head>

<body>
    <div id="header">
        <div class="navheader">books</div>
    </div>
    <div class="nav">
        <div class="navList">
            <div class="link"><a href="/">首页</a></div>

            <% for(let i=0;i<cataorys.length;i++){ %>
            <div class="link"><a href="/paging/<%-cataorys[i].id%>/page/1"><%-cataorys[i].classify%></a></div>
            <%}%>
           
        </div>
    <div id="content">
        <%-book.brief%>
        </div>
        <script>
            window.addEventListener('load', () => {
                let del = document.querySelector("#content > div.e-secret")
                del.parentNode.removeChild(del);
                del = document.querySelector("#content > div.article-social");
                del.parentNode.removeChild(del);
                let tab = document.querySelector('table');
                tab.parentNode.removeChild(tab);
            })
        </script>
</body>

</html>