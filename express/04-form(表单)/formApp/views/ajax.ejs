<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <script src="/javascripts/jQuery.3.5.1.min.js"></script>
    <script>
        let http = 'http://localhost:3000/searchBook?username=故宫';
        $.get(http).then((res) => {
            console.log(res);
            res.forEach((item, i) => {
                $('body').append("<h3>" + item.id + "</h3>" + "<h1>" + item.name + "</h2>" + "<h2>" +
                    item.author + "</h1>")
            });
        })
    </script>
</body>

</html>